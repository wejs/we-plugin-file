{
  "name": "we-plugin-file",
  "description": "We.js file plugin with suport to storages",
  "version": "2.0.4",
  "main": "plugin.js",
  "dependencies": {
    "multer": "1.3.0",
    "mv": "^2.1.1",
    "string": "^3.3.3"
  },
  "scripts": {
    "test": "NODE_ENV=test LOG_LV=info ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js -b ",
    "verbose-test": "NODE_ENV=test LOG_LV=verbose ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js -b ",
    "coverage": "NODE_ENV=test LOG_LV=info node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports -R spec test/bootstrap.js test/**/*.test.js"
  },
  "author": "Alberto Souza <contato@albertosouza.net>",
  "license": "MIT",
  "devDependencies": {
    "express-mysql-session": "^1.2.3",
    "fs-extra": "^5.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.1",
    "mysql2": "^1.5.2",
    "rimraf": "^2.6.2",
    "sinon": "^4.4.1",
    "supertest": "^3.0.0",
    "we-core": "^2",
    "we-plugin-acl": "wejs/we-plugin-acl",
    "we-plugin-file-local": "^1.2.0",
    "we-plugin-user": "^2",
    "we-test-tools": "wejs/we-test-tools"
  },
  "repository": "wejs/we-plugin-file",
  "keywords": [
    "wejs-plugin"
  ],
  "wejs": {
    "plugins": {
      "we-plugin-acl": true,
      "we-plugin-user": true,
      "we-plugin-file-local": true
    },
    "devPlugins": {}
  }
}
